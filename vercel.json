{
  "devCommand": "npm run dev",
  "builds": [
    {
      "src": "src/app/server.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/src/app/server.ts"
    }
  ],
  "env": {
    "DATABASE_URL": "mysql://root:@localhost:3306/mydb?schema=public"
  }
}
